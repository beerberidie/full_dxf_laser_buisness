# Provider selection
AI_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=sk-...          # put your key here
OPENAI_BASE_URL=https://api.openai.com/v1

# Azure OpenAI (if AI_PROVIDER=azure)
AZURE_OPENAI_ENDPOINT=https://<your>.openai.azure.com
AZURE_OPENAI_API_KEY=...
AZURE_OPENAI_DEPLOYMENT_GPT4O=mini-gpt
AZURE_OPENAI_DEPLOYMENT_EMBED=text-embedding-small

# Defaults
DEFAULT_MODEL=gpt-4.1-mini
EMBEDDING_MODEL=text-embedding-3-small
IMAGE_MODEL=gpt-image-1
TTS_VOICE=alloy

# Postgres + pgvector
DATABASE_URL=postgresql+psycopg://user:pass@localhost:5432/ai

# MinIO / S3
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=ai-module

LOG_LEVEL=INFO
ENABLE_TRACING=false
